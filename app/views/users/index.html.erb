<div id="chapter-container">
  <div class="row">
    <div class="col-lg-9 topper">

      <ul>
     <% if current_user.is_admin? %>
          <% @users.each do |user| %>
                  <li class="bottomer"><%= link_to(user.username, user, :class => "btn-signin") %>        By <%= user.email %>   |   <%= user.gender %></li>
                   <li class="bottomer">Birthday<%= user.birthday %>        By <%= user.timer %></li>
            <% end %>

      <% end %>

      </ul>

    </div>
  </div>
</div>